{
  "name": "dawidholka/laravel-polish-payments",
  "authors": [
    {
      "name": "Dawid Holka"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "illuminate/http": "^9.21",
    "illuminate/support": "^9.21",
    "spatie/laravel-data": "^2.2",
    "omnipay/common": "^3"
  },
  "require-dev" : {
    "fakerphp/faker" : "^1.14",
    "orchestra/testbench" : "^7.6",
    "pestphp/pest" : "^1.22",
    "pestphp/pest-plugin-laravel" : "^1.3",
    "phpunit/phpunit" : "^9.3"
  },
  "autoload": {
    "psr-4": {
      "DH\\PolishPayments\\": "src"
    }
  },
  "autoload-dev" : {
    "psr-4" : {
      "DH\\PolishPayments\\Tests\\" : "tests"
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "extra" : {
    "laravel" : {
      "providers" : [
        "DH\\PolishPayments\\PolishPaymentsServiceProvider"
      ]
    }
  }
}
