{
  "name": "course-link/laravel-polish-payments",
  "type": "library",
  "description": "This package allows you to take advantage of Polish payment systems.",
  "keywords": [
    "polish",
    "payments",
    "laravel",
    "przelewy24",
    "tpay",
    "paynow",
    "payu",
    "imoje",
    "gateway",
    "merchant",
    "omnipay",
    "pay",
    "payment"
  ],
  "homepage": "https://github.com/Course-Link/laravel-polish-payments",
  "license": "MIT",
  "authors": [
    {
      "name": "Dawid Holka"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "course-link/omnipay-polish-payments": "^0.0.3",
    "illuminate/http": "^9.21|^10.0",
    "illuminate/support": "^9.21|^10.0",
    "omnipay/common": "^3"
  },
  "require-dev": {
    "fakerphp/faker": "^1.14",
    "orchestra/testbench": "^7.6|^8.0",
    "pestphp/pest": "^1.22",
    "pestphp/pest-plugin-laravel": "^1.3",
    "phpunit/phpunit": "^9.3"
  },
  "autoload": {
    "psr-4": {
      "CourseLink\\Payments\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CourseLink\\Payments\\Tests\\": "tests"
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "OmnipayServiceProvider"
      ]
    }
  },
  "minimum-stability" : "dev",
  "prefer-stable" : true
}
